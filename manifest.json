{
  "name": "Google Meet Matrix Rain",
  "version": "1.0",
  "author": "Xing (x-ing.space)",
  "description": "You don't even see the code. All you see is yourself.",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://meet.google.com/*"],
      "js": ["script.webextension.js"]
    }
  ],
  "page_action": {
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}
